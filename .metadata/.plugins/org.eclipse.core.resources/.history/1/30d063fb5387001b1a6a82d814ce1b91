package br.com.rubinhocar.teste;

import br.com.rubinhocar.modelo.Veiculo;

public class TesteVeiculo {
	public static void main(String[] args) {
		
		/* Instanciar um objeto */
		 
		Veiculo carro = new Veiculo();
		
		carro.preencherMontadora("fiat");
		carro.preencherModelo("Argo");
		carro.preencherValor((float)50400.00);
		
		System.out.println(carro.retornaMontadora());
		
		
		
		
		
	}

}
